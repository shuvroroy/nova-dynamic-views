{
    "name": "shuvroroy/nova-dynamic-views",
    "description": "A tool for easier overwriting custom-header and toolbars in Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Shuvro Roy",
            "email": "shuvro.nsu.cse@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/nova": "^5.5.4"
    },
    "require-dev": {
        "laravel/nova-devtool": "^1.1",
        "laravel/pint": "^1.18"
    },
    "repositories": [
        { "type": "composer", "url": "https://nova.laravel.com" }
    ],
    "autoload": {
        "psr-4": {
            "ShuvroRoy\\NovaDynamicViews\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShuvroRoy\\NovaDynamicViews\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
